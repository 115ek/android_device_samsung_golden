+++ b/AndroidManifest.xml
@@ -2939,6 +2939,6 @@
                 <category android:name="com.android.settings.SHORTCUT" />
             </intent-filter>
         </activity>
-
+        <activity android:name=".Sponsors" />
     </application>
 </manifest>
diff --git a/res/values/strings.xml b/res/values/strings.xml
index 6a5d5d8..9c9ce9e 100644
--- a/res/values/strings.xml
+++ b/res/values/strings.xml
@@ -7054,4 +7054,11 @@
 
     <!-- Toast message letting the user know the color temperature setting is not immediate -->
     <string name="color_temperature_toast">To apply color change, turn off screen</string>
+    <!-- CODINALTEPARTS -->
+    <string name="development_name">Meticulus Development</string>
+    <string name="extras_section_name">Extras</string>
+    <string name="kernel_header">KERNEL</string> 
+    <string name="action_settings">Settings</string>
+    <string name="title_activity_sposnsors">Sposnsors</string>
+
 </resources>
diff --git a/res/xml/device_info_settings.xml b/res/xml/device_info_settings.xml
index 63fd94a..984e06a 100644
--- a/res/xml/device_info_settings.xml
+++ b/res/xml/device_info_settings.xml
@@ -82,6 +82,13 @@
             android:fragment="com.android.settings.contributors.ContributorsCloudFragment" >
         </PreferenceScreen>
 
+        <PreferenceScreen android:key="sponsors"
+                android:title="Meticulus Development Sponsors">
+            <intent android:action="android.intent.action.MAIN"
+                    android:targetPackage="com.android.settings"
+                    android:targetClass="com.android.settings.Sponsors" />
+        </PreferenceScreen>
+
         <!-- Device name -->
         <Preference android:key="device_name"
                 style="?android:preferenceInformationStyle"
